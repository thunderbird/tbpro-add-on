{
  "extends": "tbpro-shared/tsconfig.client.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@addon/*": ["./src/*"],
      "server/*": ["../backend/src/*"],
      "prisma/*": ["../backend/node_modules/@prisma/*"],
      "@send-frontend/*": ["node_modules/send-frontend/src/*"]
    },
    "types": [
      "vite/client",
      "vitest/globals",
      "@types/firefox-webext-browser",
      "@trpc/server"
    ]
  },
  "exclude": ["node_modules", "dist", "build"]
}
