{
  "name": "assist-frontend",
  "version": "0.2.4-alpha.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-prod": "run-p type-check \"build-only-prod {@}\" --",
    "build-staging": "run-p type-check \"build-only-staging {@}\" -- ",
    "build-dev": "run-p type-check \"build-only-dev {@}\" -- ",
    "build-only-prod": "NODE_ENV=production ./scripts/build.sh",
    "build-only-staging": "NODE_ENV=staging ./scripts/build.sh",
    "build-only-dev": "NODE_ENV=development ./scripts/build.sh",
    "build:xpi": "pnpm update-manifest && pnpm build-prod && ./scripts/package.sh",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "submit": "./scripts/submit.sh",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force",
    "update-manifest": "./scripts/update-manifest.sh",
    "watch": "./scripts/watch.sh"
  },
  "dependencies": {
    "@sentry/vite-plugin": "^2.22.5",
    "@thunderbirdops/services-utils": "^0.1.3",
    "@types/thunderbird-webext-browser": "^127.0.0",
    "marked": "^15.0.8",
    "pinia": "^2.1.7",
    "posthog-js": "^1.214.0",
    "vue": "^3.4.29",
    "vue-router": "4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@sentry/vue": "^8.34.0",
    "@thunderbirdops/services-ui": "^0.6.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "gpt-tokenizer": "^2.1.2",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "segmenter": "^2.0.0",
    "simple-statistics": "^7.8.5",
    "striptags": "^3.2.0",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21",
    "wink-eng-lite-web-model": "github:radishmouse/wink-eng-lite-web-model#main",
    "wink-naive-bayes-text-classifier": "^2.2.1",
    "wink-nlp": "^2.3.0"
  },
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
