{
  "name": "send-frontend",
  "version": "6.6.0",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production ./scripts/build.sh",
    "build:dev": "./scripts/build.sh",
    "build:dev:watch": "nodemon -e vue,ts,js,json --ignore 'dist*/' --ignore manifest.json --watch './{src,public}/**/*' --exec 'pnpm build:dev'",
    "build:watch": "nodemon -e vue,ts,js,json --watch './{src,public}/**/*' --exec 'pnpm build'",
    "ci:validate": "pnpm lint:all; pnpm prettier:all; pnpm run test; pnpm run typecheck",
    "deploy-xpi": "bun run ./scripts/deploy_xpi.ts",
    "dev": "vite --host",
    "lint:all": "pnpx eslint src/**/*.ts --fix",
    "prettier:all": "prettier --write .",
    "preview": "vite preview",
    "release": "./scripts/release.sh",
    "sort-package-json": "sort-package-json",
    "test": "VITE_TESTING=true vitest run --silent",
    "test-debug": "VITE_TESTING=true vitest --inspect-brk --single-thread",
    "test:watch": "VITE_TESTING=true vitest --watch",
    "typecheck": "tsc --noEmit",
    "postversion": "bun run ./scripts/bump.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@scure/bip39": "^1.5.4",
    "@sentry/vite-plugin": "^5.0.0",
    "@sentry/vue": "^8.55.0",
    "@tabler/icons-vue": "^2.47.0",
    "@tanstack/vue-query": "*",
    "@thunderbirdops/services-ui": "^1.3.0",
    "@trpc/client": ">=11.1.1",
    "@types/ua-parser-js": "^0.7.39",
    "dayjs": "*",
    "floating-vue": "^5.2.2",
    "get-random-values": "^3.0.0",
    "install": "^0.13.0",
    "jszip": "^3.10.1",
    "oidc-client-ts": "^3.3.0",
    "pinia": "^2.3.1",
    "posthog-js": "^1.235.0",
    "pretty-bytes": "^6.1.1",
    "tbpro-shared": "workspace:*",
    "ua-parser-js": "^1.0.40",
    "vue": "^3.5.13",
    "vue-final-modal": "^4.5.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@tailwindcss/forms": "^0.5.10",
    "@trpc/server": ">=11.8.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "*",
    "@types/thunderbird-webext-browser": "^127.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "*",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.1",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.8.0",
    "form-data": ">=4.0.4",
    "globals": "^17.3.0",
    "happy-dom": "^15.11.7",
    "jsonwebtoken": "9.0.2",
    "lint-staged": "^15.5.0",
    "mock-socket": "^9.3.1",
    "msw": "^2.7.3",
    "nodemon": "*",
    "postcss": "^8.5.3",
    "postcss-import": "^15.1.0",
    "prettier": "^3.8.1",
    "sort-package-json": "*",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.56.1",
    "typescript-language-server": "^3.3.2",
    "vite": ">=6.4.1",
    "vitest": "*",
    "vitest-localstorage-mock": "^0.0.1",
    "vitest-websocket-mock": "^0.4.0",
    "vue-eslint-parser": "^10.4.0"
  },
  "engines": {
    "node": ">=22.11.0"
  }
}
